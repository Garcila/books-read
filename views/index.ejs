<% include partials/header %>

<h1>Index Page</h1>

<% books.forEach(function(book){ %>
  <h1><%= book.title %></h1>
  <img src="<%= book.image %>" alt="" />
  <p>Published: <%= book.published.toDateString() %></p>
  <p><%= book.body %></p>
  <p>
    <%= book.body %><span>Rating <%= book.rating%> stars out of 5</span>
  </p>
<% }) %>

<% include partials/footer %>
